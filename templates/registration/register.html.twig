    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Alatsi&display=swap">
    <link rel="stylesheet" href="{{asset("back/fonts/fontawesome-all.min.css")}}">
    <link rel="stylesheet" href="{{asset("back/fonts/font-awesome.min.css")}}">
    <link rel="stylesheet" href="{{asset("back/fonts/ionicons.min.css")}}">
    <link rel="stylesheet" href="{{asset("back/fonts/fontawesome5-overrides.min.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/Add-Another-Button.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/Contact-Form-v2-Modal--Full-with-Google-Map.css")}}">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">
    <link rel="stylesheet" href="{{asset("back/css/Login-Form-Dark.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/Ludens---Country-State-City-selects.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/Map-Clean.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/Newsletter-Subscription-Form.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/project-card.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/Registration-Form-with-Photo.css")}}">
    <link rel="stylesheet" href="{{asset("back/css/Select2-JS-Plugin.css")}}">
 
  
        {% endblock %}

         </head>
 {% block body %}
<body style="background: rgb(25,29,40);">
 <section class="register-photo" style="background: rgba(255,255,255,0);color: rgb(15,10,35);">
        <div class="form-container"  >
        
                  {{ form_start(registrationForm,{'attr': {'novalidate': 'novalidate', 'style': 'background: rgb(30,40,51);'}}) }}
           <form  method="{{ registrationForm.vars.method }}" action="{{ registrationForm.vars.action }}" >
               
                <h1 class="text-center"><strong>Inscription </strong> </h1>
                <br><br>
               
                   <h4 class="text-center"><strong>Identite </strong> </h4>
                <div class="mb-3" > {{ form_row(registrationForm.nom) }}
</div>
                <div class="mb-3">  {{ form_row(registrationForm.prenom) }}
</div>
                <div class="mb-3"> {{ form_row(registrationForm.email) }}
</div>
<br>
<h4 class="text-center"><strong>Cordonnées </strong> </h4>
                <div class="mb-3" >  {{ form_row(registrationForm.adresse) }}
</div>
                <div class="mb-3">  {{ form_row(registrationForm.tel) }}
</div>
                <div class="mb-3">   {{ form_row(registrationForm.numCnam) }}
</div>
                <div class="mb-3"> {{ form_row(registrationForm.password, {
            label: 'Mot de passe'
        }) }}</div>
                <div class="mb-3">    {{ form_row(registrationForm.consent) }}
</div>
                <div class="mb-3">
                    
                <div class="mb-3"><button class="btn btn-primary d-block w-100" type="submit" style="background: rgb(33,74,128);">S'inscrire</button>
                </div>
            
            <a class="already" href="{{ path('app_login') }}">Avez vous déjà un compte? Connectez-vous ici.</a>
              
            </form>
             {{ form_end(registrationForm) }}
             
           {#   {{ form_errors(registrationForm) }}  #} 
       
        </div>
    </section>
     {% endblock %}

    {% block javascripts %}

    <script src="{{asset("back/bootstrap/js/bootstrap.min.js")}}"></script>
    <script src="{{asset("back/js/chart.min.js")}}"></script>
    <script src="{{asset("back/js/bs-init.js")}}"></script>
    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
    <script src="{{asset("back/js/Select2-JS-Plugin-1.js")}}"></script>
    <script src="{{asset("back/js/Select2-JS-Plugin.js")}}"></script>
    <script src="{{asset("back/js/Contact-Form-v2-Modal--Full-with-Google-Map.js")}}"></script>
    <script src="{{asset("back/js/Dynamically-Add-Remove-Table-Row.js")}}"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://geodata.solutions/includes/countrystate.js"></script>
    <script src="{{asset("back/js/theme.js")}}"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css">

     {% endblock %}
         </body>
</html>
